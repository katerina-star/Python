my_list = [7, 5, 3, 3, 2]
print(my_list)
new_element = int(input('Введите новое число в рейтинг: '))
if new_element >= 7:
    my_list.insert(0, new_element)
if new_element == 6:
    my_list.insert(1, new_element)
if 5 > new_element > 3:
    my_list.insert(2, new_element)
if new_element == 3:
    my_list.insert(4, new_element)
if new_element < 2:
    my_list.insert(5, new_element)
print(my_list)
