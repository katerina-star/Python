from functools import reduce


def new_list():
    new_list = [el for el in range(100, 1001) if el % 2 == 0]
    my_list = []
    i = 0
    for el in range(len(new_list) - 1):
        element = new_list[i] * new_list[i + 1]
        my_list.append(element)
        i += 1
    a = reduce(lambda x, y: x + y, my_list)
    return a


print(new_list())
